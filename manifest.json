{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "permissions": [
    "http://*/", "contextMenus"
  ],
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
